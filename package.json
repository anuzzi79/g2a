{
  "name": "g2a-gherkin-to-automation",
  "version": "1.0.0",
  "type": "module",
  "description": "G2A - Convert Gherkin test cases to Cypress automation scripts",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && vite",
    "dev:backend": "node backend/server.js",
    "build": "cd frontend && vite build",
    "preview": "cd frontend && vite preview",
    "start": "npm run dev"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "openai": "^4.104.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^16.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "cypress": "^15.7.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "gherkin",
    "cypress",
    "test-automation",
    "bdd"
  ],
  "author": "",
  "license": "ISC"
}
